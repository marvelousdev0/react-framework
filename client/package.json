{
  "name": "cra-template-react-framework",
  "description": "React template to create app with react, redux, mui, styled-components",
  "version": "1.0.1",
  "private": false,
  "author": {
    "name": "Vamshi Maddur",
    "email": "maddur.vamshi@gmail.com"
  },
  "main": "template.json",
  "keywords": [
    "react",
    "create-react-app",
    "cra-template",
    "template",
    "react-redux"
  ],
  "files": [
    "template",
    "template.json"
  ],
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.4",
    "@testing-library/user-event": "^12.0.11",
    "axios": "^0.19.2",
    "clsx": "^1.1.1",
    "color": "^3.1.2",
    "i18next": "^19.5.4",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.15",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-scrollbars-custom": "^4.0.25",
    "react-toastify": "^6.0.8",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "simplebar-react": "^2.2.1",
    "styled-components": "^5.1.1",
    "styled-icons": "^10.6.0"
  },
  "scripts": {
    "clean": "rimraf node_modules package-lock.json yarn.lock",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "predeploy": "npm run build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s ./src",
    "build-storybook": "build-storybook",
    "clean-files": "rm -rf ./template/public ./template/src && rm -f ./template/.eslintrc",
    "copy-files": "cp -a ./src/. template/src && cp -a ./public/. template/public && cp .eslintrc template/",
    "prepublishOnly": "npm run clean-files && npm run copy-files"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.3",
    "@storybook/react": "^5.3.19",
    "@storybook/theming": "^5.3.19",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "prettier": "^2.0.5"
  },
  "proxy": "http://localhost:8000"
}
